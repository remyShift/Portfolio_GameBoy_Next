{
	"headers": [
		{
			"source": "/assets/img/(.*).jpg",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				},
				{
					"key": "Content-Type",
					"value": "image/jpeg"
				}
			]
		},
		{
			"source": "/assets/img/(.*).png",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				},
				{
					"key": "Content-Type",
					"value": "image/png"
				}
			]
		}
	]
}
